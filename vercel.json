{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path((?!api).*)\\.html",
      "destination": "/:path",
      "permanent": true
    },
    {
      "source": "/:path((?!api).*)\\.htm",
      "destination": "/:path",
      "permanent": true
    },
    {
      "source": "/index.htm",
      "destination": "/",
      "permanent": true
    }
  ]
}


